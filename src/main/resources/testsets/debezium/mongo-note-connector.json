{
  "name": "mongodb-note-connector",
  "config": {
    "connector.class": "io.debezium.connector.mongodb.MongoDbConnector",
    "tasks.max": "1",
    "mongodb.connection.string": "mongodb://mongo-db:27017/?replicaSet=rs0",
    "topic.prefix": "coffeenote",
    "capture.mode": "change_streams_with_pre_image",
    "snapshot.mode": "no_data",
    "database.include.list": "test",
    "collection.include.list": "test.notes",
    "key.converter": "org.apache.kafka.connect.storage.StringConverter",
    "value.converter": "org.apache.kafka.connect.json.JsonConverter",
    "value.converter.schemas.enable": "false",
    "transforms": "route",
    "transforms.route.type": "org.apache.kafka.connect.transforms.RegexRouter",
    "transforms.route.regex": "coffeenote.test.notes",
    "transforms.route.replacement": "coffeenote.note.change"
  }
}
